  "standard": {
    "globals": [
      "fetch",
      "FormData",
      "it",
      "state",
      "URL"
    ],
    "parser": "babel-eslint"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.0"
  }
}
